{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tags"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "frontend",
        "backend",
        "fullstack",
        "others",
        "skill",
        "general",
        "science",
        "electronic-tech",
        "electrical"
      ]
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "tags"
    }
  }
}
